/* pages/selectProduct/selectProduct.wxss */
page{
  height:100%;
}
.wrapper{
  height:100%;
  background-color:#f5f5f5;
}
.series-name{
  color: #666;
  font-size: 28rpx;
  height:60rpx;
  padding-left:30rpx;
  line-height:60rpx;
}
.product-name{
  color:#333;
  font-size:28rpx;
  line-height:40rpx;
  word-wrap: break-word;
  word-break: break-all;
  padding: 30rpx 40rpx;
  border-bottom:1px solid #eee;
  background-color:#fff;
}
.product-name:last-child{
  border:none;
}
.shoose{
   color: #1571e5;
}
.selected{
  color:#999;
}