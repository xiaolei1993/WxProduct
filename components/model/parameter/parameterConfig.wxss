.parameter-config{
  margin-top: 20rpx;
  background: #fff;
}
.config-title{
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #e5e5e5;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
}
.parameter-list{
  display: flex;
  flex-direction: row;
}
.key{
  width: 210rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-bottom: 2rpx solid #e5e5e5;
  border-right: 2rpx solid #e5e5e5;
  background: #f9f9f9;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.key text{
  font-size: 28rpx;
  color: #333;
}
.value{
  display: flex;
  flex: 1;
  height: 82rpx;
  line-height: 82rpx;
  padding-left: 20rpx;
  border-bottom: 2rpx solid #e5e5e5;
  box-sizing: border-box;
}
.value text{
  font-size: 28rpx;
  color: #333; 
}
.load-more{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}
.load-more:after{
    margin-left: 10rpx;
    content: "\53bb";
    font-family: "detail";
    font-size: 20rpx;
}