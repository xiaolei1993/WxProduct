.switch-model{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #f5f5f5;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  z-index: 12;
}
.switch-title{
  position: relative;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #eee;
  padding: 0 150rpx;
  text-align: center;
  background: #fff;
}
.switch-title .back{
  position: absolute;
  top: 0;
  left: 0;
  width: 88rpx;
  height: 88rpx;
  text-align: center;
  font-family: "detail";
  color: #999;
}
.switch-title text{
  font-size: 36rpx;
  color: #333;
}
.switch-options{
  background: #fff;
  padding: 10rpx;
  overflow: hidden;
}
.options-list{
  float: left;
  width: 33.3%;
}
.options-list > text{
  display: block;
 margin: 10rpx;
 height: 60rpx;
 line-height: 60rpx;
 text-align: center;
 color: #333;
 font-size: 24rpx;
 border-radius: 6rpx;
 border: 2rpx solid #eee; 
}
.options-list > .select-option{
  color:#586c94;
  border:2rpx solid #586c94;
}
.switch-model-content{
  background: #fff;
  padding: 0 30rpx;
  margin-top: 20rpx;
}
.switch-model-list{
  padding: 30rpx;
  border-bottom: 2rpx solid #eee;
}
.switch-model-list:last-child{
  border: none;
}
.model-name > text{
  font-size: 28rpx;
  color: #333;
  word-wrap: break-word;
  word-break: break-all;
}
.switch-tags{
  overflow: hidden;
}
.tags-wrapper > text{
  font-size: 24rpx;
  color: #999;
  margin-right: 20rpx;
}
.sell-type{
  float: right;
  color: #f60;
  font-size: 24rpx;
}